  <hr />
  <!-- 项目名称栏以下 -->
  <div class="row" id="stigmod-workspace">
    <!-- 左侧导航栏 -->
    <div class="col-xs-3" id="stigmod-nav-left">
      <!--div class="panel panel-default"-->
        <div class="list-group">
          <!-- 搜索栏 -->
          <div class="list-group-item">
            <div class="input-group">
              <input type="text" class="form-control" placeholder="Search">
              <span class="input-group-btn">
                <button class="btn btn-default" type="button" id="btn-search"><span class="glyphicon glyphicon-search"></span></button>
              </span>
            </div>
          </div>
          <!-- 竖排导航栏（分为Class、Relation两个panel） -->
          <div class="list-group-item" id="stigmod-nav-left-scroll">
            <div class="panel panel-default">
              <div class="panel-heading stigmod-hovershow-trig">
                <h3 class="panel-title">
                  <span class="glyphicon glyphicon-th-large stigmod-classname-icon"></span>
                  <span class="stigmod-classname-title">CLASS</span>
                  <a href="#" class="stigmod-hovershow-cont"><span class="glyphicon glyphicon-plus-sign pull-right" data-toggle="tooltip" data-placement="left" data-original-title="Add a new class"></span></a>
                </h3>
              </div>
              <div class="list-group">
                <a href="template_workspace" class="list-group-item">Course<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">CourseActivity<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">CourseAvailability<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Date<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Course<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">CourseActivity<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">CourseAvailability<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Date<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Course<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">CourseActivity<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">CourseAvailability<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Date<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
              </div>
            </div>
            <!--hr class="stigmod-hr-narrow" /-->
            <div class="panel panel-default">
              <div class="panel-heading stigmod-hovershow-trig">
                <h3 class="panel-title">
                  <span class="glyphicon glyphicon-random stigmod-classname-icon"></span>
                  <span class="stigmod-classname-title">RELATION GROUP</span>
                  <a href="#" class="stigmod-hovershow-cont"><span class="glyphicon glyphicon-plus-sign pull-right" data-toggle="tooltip" data-placement="left" data-original-title="Add a new relation"></span></a>
                </h3>
              </div>
              <div class="list-group">
                <a href="#" class="list-group-item active">Course-CourseActivity<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Date-SchoolCalender<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Course-CourseActivity<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Date-SchoolCalender<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Course-CourseActivity<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Date-SchoolCalender<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Course-CourseActivity<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
                <a href="#" class="list-group-item">Date-SchoolCalender<span class="glyphicon glyphicon-chevron-right pull-right"></span></a>
              </div>
            </div>
          </div>
        </div>
      <!--/div-->
    </div>
    <!-- 右侧内容栏 -->
    <div class="col-xs-6">
      <!-- 类、名称 -->
      <div class="list-group">
        <div class="list-group-item" id="stigmod-cont-right-scroll">
          <div class="row stigmod-clickedit-root">
            <div class="col-xs-8" id="stigmod-classname">
              <span class="stigmod-keepinline">
                <span class="glyphicon glyphicon-random" id="stigmod-classname-icon"></span>
                <span id="stigmod-classname-title"> RELATION GROUP</span>
                <span> |&nbsp;</span>
              </span>
              <span class="stigmod-clickedit-text">Course-CourseActivity</span>
            </div>
            <div class="col-xs-4">
              <div class="btn-group pull-right">
                <button type="button" class="btn btn-default" data-toggle="modal" data-target="#stigmod-modal-addrelation">
                  <span class="glyphicon glyphicon-plus" data-toggle="tooltip" data-placement="left" data-original-title="Add a new attribute"></span>
                </button>
                <button type="button" class="btn btn-default stigmod-clickedit-btn-edit">
                  <span class="glyphicon glyphicon-edit" data-toggle="tooltip" data-placement="left" data-original-title="Edit class name"></span>
                </button>
                <div class="btn-group">
                  <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                    <span class="glyphicon glyphicon-cog" data-toggle="tooltip" data-placement="left" data-original-title="Configurations"></span>
                  </button>
                  <ul class="dropdown-menu dropdown-menu-right" role="menu">
                    <li><a href="#">Delete this relation group</a></li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
          <hr class="stigmod-hr-narrow"/>
          <div id="stigmod-cont-right">
            <!-- 属性 -->
            <div class="panel-heading">
              <p id="stigmod-attri-title"><span class="fa fa-book"></span>&nbsp; RELATION</p>
            </div>
            <div class="panel panel-default">
              <div class="panel-heading stigmod-hovershow-trig" data-toggle="collapse" data-target="#collapse1">
                <div class="panel-title stigmod-cursor-pointer">
                  <div class="row">
                    <div class="col-xs-2 stigmod-attri-cont-left-title"><span class="fa fa-bookmark"></span></div>
                    <div class="col-xs-6 stigmod-attri-cont-middle-title">Course [1] ----- [*] CourseActivity</div>
                    <div class="col-xs-4 stigmod-attri-cont-right-title">
                      <div class="stigmod-hovershow-cont">
                        <span><span class="glyphicon glyphicon-edit" data-toggle="tooltip" data-placement="top" data-original-title="Edit this relation"></span></span>
                        <span><span class="glyphicon glyphicon-trash" data-toggle="tooltip" data-placement="top" data-original-title="Delete this relation"></span></span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div id="collapse1" class="panel-collapse collapse">
                <table class="table stigmod-table-relation">
                  <tbody>
                    <tr>
                      <td>type</td>
                      <td>composition</td>
                      <td></td>
                    </tr>
                    <tr>
                      <td>role</td>
                      <td>whole</td>
                      <td>part</td>
                    </tr>
                    <tr>
                      <td>class</td>
                      <td>Course</td>
                      <td>CourseActivity</td>
                    </tr>
                    <tr>
                      <td>multiplicity</td>
                      <td>1..*</td>
                      <td>*</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
            <div class="panel panel-default">
              <div class="panel-heading stigmod-hovershow-trig" data-toggle="collapse" data-target="#collapse2">
                <div class="panel-title stigmod-cursor-pointer">
                  <div class="row">
                    <div class="col-xs-2 stigmod-attri-cont-left-title"><span class="fa fa-bookmark"></span></div>
                    <div class="col-xs-6 stigmod-attri-cont-middle-title">Course [1] ----- [*] CourseActivity</div>
                    <div class="col-xs-4 stigmod-attri-cont-right-title">
                      <div class="stigmod-hovershow-cont">
                        <span><span class="glyphicon glyphicon-edit" data-toggle="tooltip" data-placement="top" data-original-title="Edit this relation"></span></span>
                        <span><span class="glyphicon glyphicon-trash" data-toggle="tooltip" data-placement="top" data-original-title="Delete this relation"></span></span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div id="collapse2" class="panel-collapse collapse">
                <table class="table stigmod-table-relation">
                  <tbody>
                    <tr>
                      <td>type</td>
                      <td>composition</td>
                      <td></td>
                    </tr>
                    <tr>
                      <td>role</td>
                      <td>whole</td>
                      <td>part</td>
                    </tr>
                    <tr>
                      <td>class</td>
                      <td>Course</td>
                      <td>CourseActivity</td>
                    </tr>
                    <tr>
                      <td>multiplicity</td>
                      <td>1..*</td>
                      <td>*</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
            <div class="list-group">
              <div class="list-group-item text-center stigmod-cursor-pointer" data-toggle="modal" data-target="#stigmod-modal-addrelation">
                <a>Add a New Relation</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- 右侧推荐栏 -->
    <div class="col-xs-3" id="stigmod-rcmd-right">
      <!--div class="panel panel-default"-->
        <div class="list-group">
          <div class="list-group-item" id="stigmod-rcmd-right-scroll">
            <div class="row">
              <div class="col-xs-12" id="stigmod-rcmd-rcmd">
                <span class="glyphicon glyphicon-fire"></span>
                <span>RECOMMENDATION</span>
              </div>
            </div>
            <hr class="stigmod-hr-narrow"/>
            <div class="panel panel-default">
              <div class="panel-heading">
                <div class="panel-title stigmod-rcmd-title">CLASS</div>
              </div>
              <div class="list-group">
                <a class="list-group-item stigmod-cursor-pointer" data-toggle="modal" data-target="#stigmod-modal-addclass-disp">Course</a>
                <a href="#" class="list-group-item">CourseActivity</a>
                <a href="#" class="list-group-item">CourseAvailability</a>
                <a href="#" class="list-group-item">Date</a>
              </div>
            </div>
            <div class="panel panel-default">
              <div class="panel-heading">
                <div class="panel-title stigmod-rcmd-title">ATTRIBUTE</div>
              </div>
              <div class="list-group">
                <a href="#" class="list-group-item">date</a>
                <a href="#" class="list-group-item">time</a>
                <a href="#" class="list-group-item">teacher</a>
                <a href="#" class="list-group-item">place</a>
                <a href="#" class="list-group-item">numberStudent</a>
              </div>
            </div>
            <div class="panel panel-default">
              <div class="panel-heading">
                <div class="panel-title stigmod-rcmd-title">PROPERTY</div>
              </div>
              <div class="list-group">
                <a href="#" class="list-group-item">name : teacher</a>
                <a href="#" class="list-group-item">type : String</a>
                <a href="#" class="list-group-item">multiplicity : 1</a>
                <a href="#" class="list-group-item">visiblity : true</a>
              </div>
            </div>
          </div>
        </div>
      <!--/div-->
    </div>
  </div>
</div>
